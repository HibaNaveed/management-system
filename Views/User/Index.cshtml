
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<main class="main">

    <!-- Hero Section -->
    <section id="hero" class="hero section dark-background">

        <div id="hero-carousel" class="carousel slide carousel-fade" data-bs-ride="carousel" data-bs-interval="5000">

            <div class="carousel-item active">
                <img src="~/Restaurant/assets/img/hero-carousel/hero-carousel-1.jpg" alt="">
                <div class="carousel-container">
                    <h2><span>Saffron & Silk</span> Restaurant</h2>
                    <p>where tradition meets elegance on a plate. A celebration of rich flavors, aromatic spices, and culinary artistry, our restaurant brings you a dining experience woven with warmth and indulgence.</p>
                    <div>
                        <a href="@Url.Action("Menu","User")" class="btn-get-started">Our Menu</a>
                        <a href="@Url.Action("Bookatable","User")" class="btn-get-started">Book a table</a>
                    </div>
                </div>
            </div><!-- End Carousel Item -->

            <div class="carousel-item">
                <img src="~/Restaurant/assets/img/hero-carousel/hero-carousel-2.jpg" alt="">
                <div class="carousel-container">
                    <h2>A Taste of Tradition, A Touch of Elegance</h2>
                    <p>Inspired by the richness of saffron and the elegance of silk, we bring you a dining journey that blends tradition with modern sophistication</p>
                    <div>
                        <a href="#menu" class="btn-get-started">Our Menu</a>
                        <a href="@Url.Action("Bookatable","User")" class="btn-get-started">Book a table</a>
                    </div>
                </div>
            </div><!-- End Carousel Item -->

            <div class="carousel-item">
                <img src="~/Restaurant/assets/img/hero-carousel/hero-carousel-3.jpg" alt="">
                <div class="carousel-container">
